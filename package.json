{
    "name": "@ver01/form-theme-antd",
    "version": "1.0.7-alpha.1",
    "description": "Ver01 Form Theme Antd",
    "keywords": [
        "ver01.com",
        "@ver01/form-theme-antd"
    ],
    "scripts": {
        "lint": "eslint src",
        "cs-format": "prettier 'src/**/*.js' --write",
        "precommit": "npm run lint && npm run cs-format && git add .",
        "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js",
        "build:readme": "toctoc README.md -w",
        "build:libjs": "cross-env NODE_ENV=production babel -d lib/ src/",
        "build:libcss": "node-sass src/index.scss lib/index.css --importer node_modules/node-sass-import",
        "build:lib": "rimraf lib && npm run build:libjs && npm run build:libcss",
        "cs-check": "prettier 'src/**/*.js' --check",
        "publish-to-npm": "npm run build:readme && npm run build:dist && npm run build:lib && npm publish",
        "preversion": "npm run build:readme && npm run build:dist && npm run build:lib && npm run cs-check && npm run lint"
    },
    "main": "dist/index.js",
    "files": [
        "dist",
        "lib"
    ],
    "peerDependencies": {
        "antd": "*",
        "moment": "*",
        "react": "*"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.0",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "antd": "^3.26.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "cross-env": "^6.0.3",
        "eslint": "^6.7.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "fibers": "3.1.0",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "sass": "^1.23.7",
        "sass-loader": "^8.0.0",
        "terser-webpack-plugin": "^2.2.3",
        "toctoc": "^0.3.2",
        "webpack": "^4.41.0",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ver01/form-theme-antd.git"
    },
    "author": "Jerry<alphaformbase@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/ver01/form-theme-antd#readme"
}
